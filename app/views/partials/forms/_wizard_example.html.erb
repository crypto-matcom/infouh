  <input type="text" name="prefix[having][having1][type]" value="table">
  <input type="text" name="prefix[having][having1][value]" value="table">
  <input type="text" name="prefix[having][having1][value_type]" value="table">

  <input type="text" name="prefix[orders][order1][column]" value="table0.column0">
  <input type="text" name="prefix[orders][order1][type]" value="ASC">

  <input type="text" name="prefix[limit]" value="5">
  <%= submit_tag "Send" %>
