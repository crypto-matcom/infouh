<%= form_tag show_query_path, class: 'ui form' do %>
  <input type="text" name="prefix[tables][]" value="table1, table2, table3">

  <input type="select" name="prefix[columns][column1][column]" value="table1.column1">
  <input type="hidden" name="prefix[columns][column1][type]" value="single">

  <input type="text" name="prefix[columns][column4][column]" value="table1.column4">
